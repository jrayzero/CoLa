def note(msg):
  print('NOTE: ' + str(msg))

def clip3(x, y, z):
  if z < x:
    return x
  elif z > y:
    return y
  else:
    return z

def clip1y(x, bit_depth_Y):
  return clip3(0, (1<<bit_depth_Y)-1, x)

class MblkInfo:

  slice_id: int

  def __str__(self):
    s = ''
    s += 'slice_id: ' + str(self.slice_id)
    return s
